{
  "name": "mgm-affiliate-platform",
  "version": "0.1.0",
  "description": "Affiliate marketing platform for e-commerce SMBs",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "docker-compose up -d && nodemon src/index.js",
    "test": "jest --config=jest.config.js",
    "test:report": "jest --config=jest.config.js && node generate-test-report.js",
    "test:coverage": "COLLECT_COVERAGE=true jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:build": "docker-compose build",
    "docker:prune": "docker system prune -f",
    "docker:clean": "docker-compose down -v",
    "docker:status": "docker ps"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/migrations/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 75,
        "functions": 80,
        "lines": 80
      },
      "./src/modules/auth/": {
        "statements": 90,
        "branches": 85,
        "functions": 90,
        "lines": 90
      },
      "./src/modules/tracking/": {
        "statements": 90,
        "branches": 85,
        "functions": 90,
        "lines": 90
      }
    }
  },
